{
  "name": "linter-csslint",
  "keywords": [
    "csslint",
    "css",
    "linter",
    "lint"
  ],
  "main": "./lib/main",
  "version": "2.0.6",
  "description": "Lint CSS on the fly, using csslint",
  "repository": {
    "type": "git",
    "url": "https://github.com/AtomLinter/linter-csslint.git"
  },
  "license": "MIT",
  "private": true,
  "configSchema": {
    "executablePath": {
      "type": "string",
      "default": "",
      "description": "If unset a project local install of CSSLint is attempted to be used first, falling back to the bundled version. Requires a full path to `csslint` (e.g.: `/usr/bin/csslint` or `C:\\foo\\bar\\csslint.cmd`)."
    }
  },
  "engines": {
    "atom": ">=1.4.0 <2.0.0"
  },
  "dependencies": {
    "atom-linter": "10.0.0",
    "atom-package-deps": "5.1.0",
    "csslint": "1.0.5",
    "fs-plus": "3.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@commitlint/travis-cli": "8.3.5",
    "@semantic-release/apm-config": "8.0.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.20.1",
    "husky": "4.2.3",
    "jasmine-fix": "1.3.1",
    "semantic-release": "19.0.3"
  },
  "package-deps": [
    "linter:2.0.0"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "apm test"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "global-require": "off",
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "atom"
          ]
        }
      ]
    },
    "globals": {
      "atom": true
    },
    "env": {
      "node": true,
      "browser": true
    }
  },
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "semanticCommits": true,
    "rangeStrategy": "pin",
    "packageRules": [
      {
        "packagePatterns": [
          "^eslint"
        ],
        "groupName": "ESLint packages"
      }
    ]
  },
  "release": {
    "extends": "@semantic-release/apm-config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
