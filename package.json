{
  "name": "linter-csslint",
  "keywords": [
    "csslint",
    "css",
    "linter",
    "lint"
  ],
  "main": "./lib/main",
  "version": "1.1.0",
  "description": "Lint CSS on the fly, using csslint",
  "repository": "https://github.com/AtomLinter/linter-csslint",
  "license": "MIT",
  "dependencies": {
    "csslint": "git+https://github.com/AtomLinter/csslint",
    "atom-linter": "^4.0.1",
    "atom-package-deps": "^3.0.6"
  },
  "devDependencies": {
    "coffeelint": "^1.12.1"
  },
  "package-deps": [
    "linter"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/coffeelint lib"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
